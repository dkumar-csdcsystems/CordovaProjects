<?xml version='1.0' encoding='utf-8'?>
<widget id="com.csdcsystems.eChallan" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <name>eChallan</name>
    <description>
        eChallan Smartphone App
    </description>
    <author email="dkumar@csdcsystems.com" href="http://www.csdcsystems.com">
        CSDC Systems Inc
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
